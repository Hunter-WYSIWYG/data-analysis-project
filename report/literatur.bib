@inproceedings{tablet,
author = {Sadana, Ramik and Stasko, John},
title = {Designing and Implementing an Interactive Scatterplot Visualization for a Tablet Computer},
year = {2014},
isbn = {9781450327756},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/2598153.2598163},
doi = {10.1145/2598153.2598163},
abstract = {Tablet computers now offer screen sizes and computing capabilities that are competitive
with traditional desktop PCs. Their popularity has grown tremendously, but we are
just beginning to see information visualization applications designed for this platform.
One potential reason for this limited development is the challenge of designing and
implementing a multi-touch interface for visualizations on mobile, tablet devices.
In this work, we identify the primary challenges that touch screen interactions pose
for information visualization applications. We explore the design space of multi-touch
interactions for visualizations and present a prototype information visualization
application using a specific technique, a dynamic scatterplot, for an iPad.},
booktitle = {Proceedings of the 2014 International Working Conference on Advanced Visual Interfaces},
pages = {265–272},
numpages = {8},
keywords = {tablet computer, multi-touch interaction, scatterplot, gesture, information visualization},
location = {Como, Italy},
series = {AVI '14}
}

